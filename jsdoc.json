{
  "plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category"
  ],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/cli.ts",
      "src/services/global-variables.ts",
      "src/__tests__",
      "dist"
    ],
    "includePattern": ".+\\.(ts|tsx)$",
    "excludePattern": ".+\\.(test|spec)\\.(ts|tsx)$"
  },
  "typescript": {
    "moduleRoot": "src"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "verbose": false,
    "template": "node_modules/better-docs",
    "tutorials": "guides",
    "readme": "README.md"
  },
  "templates": {
    "search": true,
    "better-docs": {
      "name": "Flow Test Engine API",
      "logo": "public/assets/flow.png",
      "css": [
        "public/assets/custom.css"
      ],
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/marcuspmd/flow-test"
        },
        {
          "label": "Example Result",
          "href": "https://marcuspmd.github.io/flow-test/public/report/index.html"
        }
      ],
      "navigation": [
        {
          "label": "Getting Started",
          "href": "tutorial-getting-started.html"
        },
        {
          "label": "Configuration Guide",
          "href": "tutorial-configuration-guide.html"
        },
        {
          "label": "YAML Syntax Reference",
          "href": "tutorial-yaml-syntax-reference.html"
        },
        {
          "label": "CLI Reference",
          "href": "tutorial-cli-reference.html"
        },
        {
          "label": "Advanced Features",
          "href": "tutorial-advanced-features.html"
        },
        {
          "label": "Reporting Guide",
          "href": "tutorial-reporting-guide.html"
        },
        {
          "label": "Best Practices",
          "href": "tutorial-best-practices.html"
        }
      ]
    }
  }
}
