{
  "source": {
    "include": [
      "./dist/"
    ],
    "includePattern": "\\.(js)$",
    "exclude": [
      "node_modules/"
    ]
  },
  "opts": {
    "destination": "./docs/jsdoc/",
    "readme": "./docs/API_DOCUMENTATION.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}