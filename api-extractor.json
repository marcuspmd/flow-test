{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./dist/index.d.ts",
  "apiReport": {
    "enabled": true,
    "reportFolder": "./docs/api-report/",
    "reportTempFolder": "./temp/"
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      },
      "ae-forgotten-export": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      },
      "tsdoc-escape-right-brace": {
        "logLevel": "none"
      },
      "tsdoc-malformed-inline-tag": {
        "logLevel": "none"
      },
      "tsdoc-escape-greater-than": {
        "logLevel": "none"
      },
      "tsdoc-html-tag-missing-greater-than": {
        "logLevel": "none"
      }
    }
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "./temp/<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "./temp/tsdoc-metadata.json"
  }
}