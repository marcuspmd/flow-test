{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./dist/index.d.ts",
  "apiReport": {
    "enabled": true,
    "reportFolder": "./docs/api-report/",
    "reportTempFolder": "./temp/"
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": { "logLevel": "warning" },
      "ae-unresolved-link": { "logLevel": "warning" },
      "ae-forgotten-export": { "logLevel": "warning" }
    },
    "tsdocMessageReporting": {
      "tsdoc-undefined-tag": { "logLevel": "warning" },
      "tsdoc-escape-right-brace": { "logLevel": "warning" },
      "tsdoc-malformed-inline-tag": { "logLevel": "warning" },
      "tsdoc-escape-greater-than": { "logLevel": "warning" },
      "tsdoc-html-tag-missing-greater-than": { "logLevel": "warning" }
    }
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "./docs/api/flow-test-engine.api.json"
  },
  "dtsRollup": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "./temp/tsdoc-metadata.json"
  }
}
