{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/typedoc-site",
  "tsconfig": "tsconfig.json",
  
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": false,
  "customCss": "docs/typedoc-custom.css",
  "exclude": [
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/*.test.ts",
    "dist/**",
    "temp/**"
  ],
  "readme": "none",
  "name": "Flow Test Engine",
  "categorizeByGroup": true,
  "validation": {
    "invalidLink": true,
    "notDocumented": true,
    "notExported": true
  },
  "treatWarningsAsErrors": false,
  "navigationLinks": {
    "GitHub": "https://github.com/marcuspmd/flow-test"
  }
}
