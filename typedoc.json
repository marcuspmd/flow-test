{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Flow Test Engine",
  "entryPoints": ["./src/index.ts"],
  "out": "./docs/api",
  "theme": "default",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Services",
    "Types",
    "Report Generator",
    "*"
  ],
  "sort": ["source-order"],
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/node_modules/**",
    "**/dist/**",
    "**/temp/**"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "readme": "./README.md",
  "includeVersion": true,
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "titleLink": "https://github.com/marcus/flow-test-engine",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "customCss": "./docs/styles/typedoc-custom.css",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "githubPages": true,
  "hideGenerator": false,
  "searchCategoryBoosts": {
    "Core": 1.5,
    "Services": 1.2
  },
  "groupOrder": [
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Functions",
    "Variables"
  ]
}